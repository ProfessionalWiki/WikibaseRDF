{
	"name": "Wikibase RDF",
	"type": "wikibase",

	"version": "1.0.0-alpha",

	"author": [
		"[https://Professional.Wiki/ Professional.Wiki]",
		"[https://www.EntropyWins.wtf/mediawiki Jeroen De Dauw]"
	],

	"license-name": "GPL-2.0-or-later",

	"url": "https://github.com/ProfessionalWiki/WikibaseRDF",

	"descriptionmsg": "wbrdf-description",

	"requires": {
		"MediaWiki": ">= 1.37.0",
		"extensions": {
			"WikibaseRepository": "*"
		}
	},

	"MessagesDirs": {
		"WikibaseRDF": [
			"i18n"
		]
	},

	"AutoloadNamespaces": {
		"ProfessionalWiki\\WikibaseRDF\\": "src/",
		"ProfessionalWiki\\WikibaseRDF\\Tests\\": "tests/"
	},

	"Hooks": {
	},

	"config": {
	},

	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "WikibaseRDF/resources"
	},

	"ResourceModules": {
		"ext.wikibase.rdf": {
			"dependencies": [
			],
			"scripts": [
			],
			"styles": [
			]
		}
	},

	"RestRoutes": [
		{
			"path": "/wikibase-rdf/v0/get/{entity-id}",
			"method": "GET",
			"factory": "ProfessionalWiki\\WikibaseRDF\\Rest\\Handler\\GetAllMapping::factory"
		},
		{
			"path": "/wikibase-rdf/v0/get/{entity-id}/{predicate}",
			"method": "GET",
			"factory": "ProfessionalWiki\\WikibaseRDF\\Rest\\Handler\\GetMapping::factory"
		},
		{
			"path": "/wikibase-rdf/v0/get-for/{entity-id}/{object}",
			"method": "GET",
			"factory": "ProfessionalWiki\\WikibaseRDF\\Rest\\Handler\\GetMappingFor::factory"
		},
		{
			"path": "/wikibase-rdf/v0/save/{entity-id}/{predicate}/{object}",
			"method": "PUT",
			"factory": "ProfessionalWiki\\WikibaseRDF\\Rest\\Handler\\SaveMapping::factory"
		},
		{
			"path": "/wikibase-rdf/v0/save-mappings/{entity-id}",
			"method": "POST",
			"factory": "ProfessionalWiki\\WikibaseRDF\\Rest\\Handler\\SaveMappings::factory"
		}
	],
	"manifest_version": 2
}
