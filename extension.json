{
	"name": "Wikibase RDF",
	"type": "wikibase",

	"version": "1.0.0-alpha",

	"author": [
		"[https://Professional.Wiki/ Professional.Wiki]",
		"[https://www.EntropyWins.wtf/mediawiki Jeroen De Dauw]"
	],

	"license-name": "GPL-2.0-or-later",

	"url": "https://github.com/ProfessionalWiki/WikibaseRDF",

	"descriptionmsg": "wbrdf-description",

	"requires": {
		"MediaWiki": ">= 1.37.0",
		"extensions": {
			"WikibaseRepository": "*"
		}
	},

	"MessagesDirs": {
		"WikibaseRDF": [
			"i18n"
		]
	},

	"AutoloadNamespaces": {
		"ProfessionalWiki\\WikibaseRDF\\": "src/",
		"ProfessionalWiki\\WikibaseRDF\\Tests\\": "tests/"
	},

	"Hooks": {
		"OutputPageParserOutput": "ProfessionalWiki\\WikibaseRDF\\Hooks::onOutputPageParserOutput",
		"WikibaseRepoEntityTypes": "ProfessionalWiki\\WikibaseRDF\\Hooks::onWikibaseRepoEntityTypes"
	},

	"config": {
	},

	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "WikibaseRDF/resources"
	},

	"ResourceModules": {
		"ext.wikibase.rdf": {
			"dependencies": [
				"wikibase.view.ControllerViewFactory"
			],
			"scripts": [
				"ext.wikibase.rdf.js"
			],
			"styles": [
				"ext.wikibase.rdf.less"
			],
			"messages": [
				"wikibase-rdf-mappings-toggler"
			]
		}
	},

	"manifest_version": 2
}
